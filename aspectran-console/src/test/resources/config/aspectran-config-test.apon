context: {
    base: ./target/test-classes
    root: /WEB-INF/aspectran/config/test-root.xml
    encoding: utf-8
    resources: [
        /WEB-INF/aspectran/config
    ]
    profiles: {
        active: dev, stage, prod
    }
    hybridLoad: false
    autoReload: {
        reloadMode: hard
        scanIntervalSeconds: 5
        startup: false
    }
}
session: {
    timeout: 60
    evictionPolicy: -1
    saveOnCreate: false
    saveOnInactiveEviction: false
    removeUnloadableSessions: false
    storeType: file
    fileStore: {
        path: /sessions
        deleteUnrestorableFiles: false
    }
}
scheduler: {
    startDelaySeconds: 10
    waitOnShutdown: true
    startup: true
}
web: {
    uriDecoding: utf-8
}