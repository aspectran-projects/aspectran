context: {
    root: classpath:config/shell/jline/shell-test-root-config.xml
    resources: [
    ]
    hybridLoad: false
    autoReload: {
        reloadMode: soft
        scanIntervalSeconds: 5
        startup: false
    }
    profiles: {
    }
}
scheduler: {
    startDelaySeconds: 3
    waitOnShutdown: true
    startup: false
}
shell: {
    prompt: "test-console-app> "
    commands: [
        com.aspectran.shell.command.builtin.JettyCommand
        com.aspectran.shell.command.builtin.TransletCommand
        com.aspectran.shell.command.builtin.SysInfoCommand
        com.aspectran.shell.command.builtin.PBEncryptCommand
        com.aspectran.shell.command.builtin.PBDecryptCommand
        com.aspectran.shell.command.builtin.HelpCommand
        com.aspectran.shell.command.builtin.VerboseCommand
        com.aspectran.shell.command.builtin.RestartCommand
        com.aspectran.shell.command.builtin.PauseCommand
        com.aspectran.shell.command.builtin.ResumeCommand
        com.aspectran.shell.command.builtin.ClearCommand
        com.aspectran.shell.command.builtin.QuitCommand
    ]
    verbose: true
    greetings: (
        |
        |   {{bold}}--- Aspectran Demo Application ---{{bold:off}}
        |
        |{{underline}}Built-in commands for this console application:{{underline:off}}
        |   {{green}}desc on{{fg:reset}}   Prints a description of the command each time the command is executed
        |   {{green}}desc off{{fg:reset}}  Turns off print a description of the command each time the command is executed
        |   {{green}}help{{fg:reset}}      Prints help for the available commands that was displayed at startup
        |   {{green}}restart{{fg:reset}}   Restarts this console application
        |   {{green}}mem{{fg:reset}}       Shows memory usage
        |   {{green}}gc{{fg:reset}}        Performs a garbage collection
        |   {{green}}quit{{fg:reset}}      Exits from this console application
        |
    )
}